{
    "color-highlight.markerType": "underline",
    "editor.fontFamily": "'Source Code Pro Semibold', 'Dank Mono', 'Fira Code Medium', monospace, 'Droid Sans Fallback'",
    "editor.fontLigatures": true,
    "editor.fontWeight": "bold",
    "editor.minimap.enabled": false,
    "editor.renderWhitespace": "none",
    "editor.tabSize": 2,
    "emmet.triggerExpansionOnTab": true,
    "keyboard.dispatch": "keyCode",
    "search.location": "panel",
    "terminal.external.linuxExec": "termite",
    "terminal.integrated.fontSize": 15,
    "vim.easymotion": true,
    "vim.easymotionMarkerFontFamily": "Source Code Pro Semibold",
    "vim.foldfix": true,
    "window.menuBarVisibility": "toggle",
    "window.zoomLevel": 0,
    "workbench.editor.showTabs": true,
    "workbench.iconTheme": "material-icon-theme",
    "[python]": {
        "editor.tabSize": 4
    },
    "files.trimTrailingWhitespace": true,
    "workbench.colorTheme": "Material Dark Theme",
    "workbench.colorCustomizations": {},
    "editor.smoothScrolling": true,
    "editor.cursorBlinking": "phase",
    "vim.easymotionMarkerFontWeight": "bold",
    "emmet.includeLanguages": {
        "vue-html": "html",
        "vue": "html",
        "javascript": "javascriptreact"
    },
    "vim.useSystemClipboard": true,
    "material-icon-theme.folders.color": "#90a4ae",
    "terminal.integrated.fontFamily": "'Source Code Pro Semibold'",
    "vim.easymotionMarkerFontSize": "15",
    "vim.easymotionMarkerHeight": 18,
    "vim.easymotionMarkerWidthPerChar": 10,
    "vim.easymotionMarkerYOffset": 2,
    "editor.lineNumbers": "relative",
    "editor.minimap.renderCharacters": false,
    "gitProjectManager.baseProjectsFolders": [
        "~/Cloud",
        "~/Projects"
    ],
    "editor.acceptSuggestionOnCommitCharacter": false,
    "zenMode.centerLayout": false,
    "python.linting.flake8Enabled": true,
    "python.linting.flake8Path": "/usr/bin/flake8",
    "zenMode.hideTabs": false,
    "workbench.activityBar.visible": false
}